pipeline {
    agent {label 'MAVEN3&&NVM&&PYTHON3&&PIP3'} 
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Manipavan98/openmrs-core.git' , branch : 'master' 
            }
        }
        stage('install') { 
            steps {
                sh 'mvn package'
            }
        }
    }
}